modules = ["nodejs-20", "web", "postgresql-16"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"

[extension]
isExtension = true
buildCommand = "npx vite build --config vite.extension.config.ts"
outputDirectory = "dist/extension"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33579
externalPort = 3001

[[ports]]
localPort = 34055
externalPort = 3002

[[ports]]
localPort = 34703
externalPort = 6000

[[ports]]
localPort = 35127
externalPort = 5000

[[ports]]
localPort = 35755
externalPort = 3000

[[ports]]
localPort = 36595
externalPort = 3003

[[ports]]
localPort = 38391
externalPort = 5173

[[ports]]
localPort = 39811
externalPort = 4200

[[ports]]
localPort = 42491
externalPort = 6800

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[agent]
mockupState = "FULLSTACK"
